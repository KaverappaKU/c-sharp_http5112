@model IEnumerable<CumulativeProject.Models.Student>
@{
    ViewBag.Title = "List Students";
}

<a class="navlink" href="/">Back To Home</a>
<h2>ListStudents</h2>
<p>This page should be showing information about all Students</p>

<div id="containerlist">
    <h2>List of Students</h2>
    @foreach (var student in Model)
    {
        <div class="listitem row">
            <div class="col-md-12"><a href="/Student/ShowStudents/@student.StudentId">@student.StudentFname @student.StudentLname</a></div>
        </div>
    }
</div>

