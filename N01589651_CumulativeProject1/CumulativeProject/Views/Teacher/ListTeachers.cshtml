@model IEnumerable<CumulativeProject.Models.Teacher>

@{
    ViewBag.Title = "List Teachers";
}

<a class="navlink" href="/">Back To Home</a>
<h2>ListTeachers</h2>
<p>This page should be showing information about all Teachers</p>

<div id="containerlist">
    <h2>List of Teachers</h2>
    @foreach (var teacher in Model)
    {
        <div class="listitem row">
            <div class="col-md-12"><a href="/Teacher/ShowTeachers/@teacher.TeacherId">@teacher.TeacherFname @teacher.TeacherLname</a></div>
        </div>
    }
</div>
